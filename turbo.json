{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "land#build": {
      "dependsOn": ["^build", "$NEXT_PUBLIC_API_URL", "$NEXT_PUBLIC_API_URL_2"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "mobile#ios": {
      "cache": false
    },
    "land#dev": {
      "cache": false
    },
    "uim#build": {
      "outputs": ["dist/**"]
    },
    "uim#dev": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
